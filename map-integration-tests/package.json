{
  "name": "map-integration-tests",
  "description": "Runs integration tests by starting a local web server with a page that contains the SDK code.",
  "scripts": {
    "test": "jest",
    "test:coverage": "",
    "build": "",
    "build:full": "npm run type-check && npm run build",
    "start-webpack-dev-server": "webpack serve",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "format": "prettier --check \"src/**/*\"",
    "format:fix": "prettier --write \"src/**/*\"",
    "type-check": "tsc --noEmit",
    "update-dependencies": "npx npm-check-updates -u --packageFile package.json",
    "clean:dist": "rimraf ./dist",
    "clean:cache": "rimraf ./.rollup.cache",
    "clean": "npm run clean:dist && npm run clean:cache"
  },
  "author": "GO SDK JS Team",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tomtom-international/maps-sdk-js.git"
  },
  "bugs": {
    "url": "https://github.com/tomtom-international/maps-sdk-js/issues"
  },
  "homepage": "https://github.com/tomtom-international/maps-sdk-js#readme",
  "devDependencies": {
    "@types/jest": "^29.5.4",
    "@types/jest-environment-puppeteer": "^5.0.3",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "dotenv": "^16.3.1",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-jest": "^27.2.3",
    "eslint-plugin-prettier": "^5.0.0",
    "html-webpack-plugin": "^5.5.3",
    "jest": "^29.7.0",
    "jest-environment-puppeteer": "^9.0.0",
    "jest-puppeteer": "^9.0.0",
    "prettier": "^3.0.3",
    "puppeteer": "^21.2.0",
    "rimraf": "^5.0.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  }
}
