[compliance]
# Example texts of the suppressed warnings:
# Suppressing warning 301 - "No proper copyright for component `<component_name>` from project `<project_name>` defined."
# Suppressing warning 302 - "Copyright for component `<component_name>` from project `<project_name>` has improper format."
# Suppressing warning 309 - "Project has <number_of_risks> high operational risks: <risk_1>, <risk_2>, <risk_3>, <risk_4>"
# Suppressing warning 311 - "Components failed to be mapped or missing for `<project_name>`. Components not found: [<component_1>, <component_2>, <component_3>]"
# Suppressing warning 320 - "Project <project_identifier> is in a PLANNING phase and it is expected to be pre-release, released, or archived. This can indicate that the scan failed, the analysis is invalid or incomplete, or the version is not expected to be used for a release."
# Suppressing warning 322 - "Project has high overall operational risk. Please check out also unconfirmed snippets in Black Duck UI."

args = "-W 301 -W 302 -W 309 -W 311 -W 320 -W 322"

# Disclaimer:
# Suppressing warnings does not remove them from the logs. Instead, it prevents the workflow from failing due to these warnings.
# Frequently review suppressed warnings to ensure that they do not indicate critical issues that need attention.
# For more information about Black Duck warning codes: https://blackducktools.tomtomgroup.com/contents/tools/error_resolution.html#warnings