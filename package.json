{
  "name": "@anw/go-sdk-js",
  "version": "0.1.43",
  "description": "JavaScript library for TomTom maps and location services",
  "scripts": {
    "clean": "rm -rf ./*/.rollup.cache ./*/dist",
    "release:dry": "npx standard-version --dry-run",
    "release": "if [ \"$(git rev-parse --abbrev-ref HEAD)\" == \"main\" ]; then npx standard-version --commit-all && git push --follow-tags origin main; else echo \"RELEASE IS ONLY PERMITTED FROM MAIN BRANCH\"; fi",
    "documentation": "typedoc",
    "build": "npm run build:full -ws"
  },
  "publishConfig": {
    "registry": "https://tomtom.jfrog.io/artifactory/api/npm/go-sdk-js-npm-local/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tomtom-international/go-sdk-js.git"
  },
  "keywords": [
    "tomtom",
    "sdk",
    "api",
    "map",
    "location",
    "search",
    "routing",
    "traffic"
  ],
  "author": "GO SDK JS Team",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/tomtom-international/go-sdk-js/issues"
  },
  "homepage": "https://github.com/tomtom-international/go-sdk-js#readme",
  "workspaces": [
    "core",
    "services",
    "map",
    "map-integration-tests"
  ],
  "exports": {
    "./package.json": "./package.json",
    "./core": {
      "import": "./core/dist/core.es.js",
      "require": "./core/dist/core.cjs.js",
      "node": "./core/dist/core.cjs.js"
    },
    "./services": {
      "import": "./services/dist/services.es.js",
      "require": "./services/dist/services.cjs.js",
      "node": "./services/dist/services.cjs.js"
    },
    "./map": {
      "import": "./map/dist/map.es.js",
      "require": "./map/dist/map.cjs.js",
      "node": "./map/dist/map.cjs.js"
    }
  },
  "types": "./index.d.ts",
  "dependencies": {
    "@types/geojson": "^7946.0.10",
    "@types/lodash": "^4.14.185"
  },
  "devDependencies": {
    "@size-limit/file": "^8.1.0",
    "@size-limit/preset-app": "^8.1.0",
    "size-limit": "^8.1.0",
    "typedoc": "^0.23.15",
    "typedoc-plugin-markdown": "^3.13.6",
    "typedoc-plugin-missing-exports": "^1.0.0",
    "map": "file:map",
    "services": "file:services"
  },
  "size-limit": [
    {
      "path": "services/dist/services*.js"
    },
    {
      "path": "core/dist/core*.js"
    },
    {
      "path": "map/dist/map*.js"
    }
  ],
  "volta": {
    "node": "16.17.1",
    "npm": "8.19.2"
  }
}
