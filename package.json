{
  "name": "@anw/maps-sdk-js",
  "version": "0.5.1",
  "description": "JavaScript library for TomTom maps and location services",
  "scripts": {
    "clean": "npm run clean:dist -ws && npm run clean:cache -ws",
    "release:dry": "npx standard-version --dry-run",
    "release": "./scripts/release.sh && git push --follow-tags origin main",
    "documentation": "./scripts/documentation/generate-api-reference.sh",
    "build:full": "npm run build:full -ws",
    "build": "npm run build -ws",
    "type-check": "npm run type-check -ws",
    "lint": "npm run lint -ws",
    "format": "npm run format -ws",
    "test": "npm run test -ws",
    "prepare": "node scripts/prepare.js",
    "pre-commit": "lint-staged"
  },
  "publishConfig": {
    "registry": "https://tomtom.jfrog.io/artifactory/api/npm/maps-sdk-js-npm-local/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tomtom-international/maps-sdk-js.git"
  },
  "keywords": [
    "tomtom",
    "sdk",
    "api",
    "map",
    "location",
    "search",
    "routing",
    "traffic"
  ],
  "author": "GO SDK JS Team",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/tomtom-international/maps-sdk-js/issues"
  },
  "homepage": "https://github.com/tomtom-international/maps-sdk-js#readme",
  "workspaces": [
    "core",
    "services",
    "map",
    "map-integration-tests"
  ],
  "dependencies": {
    "@types/geojson": "^7946.0.13",
    "@types/lodash": "^4.14.202",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@size-limit/file": "^9.0.0",
    "@size-limit/preset-app": "^9.0.0",
    "@types/uuid": "^9.0.7",
    "cspell": "^7.3.9",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-jest": "^27.6.2",
    "eslint-plugin-prettier": "^4.2.1",
    "handlebars": "^4.7.8",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "map": "*",
    "prettier": "^2.8.8",
    "services": "*",
    "size-limit": "^9.0.0",
    "typedoc": "^0.25.7",
    "typedoc-plugin-markdown": "4.0.0-next.10",
    "typedoc-plugin-missing-exports": "^2.1.0",
    "yaml": "^2.3.4"
  },
  "size-limit": [
    {
      "path": "services/dist/services*.js"
    },
    {
      "path": "core/dist/core*.js"
    },
    {
      "path": "map/dist/map*.js"
    }
  ],
  "standard-version": {
    "scripts": {
      "postbump": "./scripts/documentation/update-sdk-version-in-docs.sh && git add typedoc.json documentation/dev-portal/"
    }
  },
  "volta": {
    "node": "21.6.1"
  }
}
