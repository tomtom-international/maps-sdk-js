{
  "name": "@anw/maps-sdk-js",
  "version": "0.8.23",
  "description": "JavaScript library for TomTom maps and location services",
  "scripts": {
    "clean": "npm run clean:dist -ws && npm run clean:cache -ws",
    "release:dry": "npx standard-version --dry-run",
    "release": "./scripts/release.sh && git push --follow-tags origin main",
    "documentation": "typedoc",
    "build:full": "npm run build:full -ws",
    "build": "npm run build -ws",
    "type-check": "npm run type-check -ws",
    "lint": "biome lint .",
    "lint:fix": "biome check . --write && biome lint . --write",
    "format": "biome format .",
    "format:fix": "biome format . --write",
    "test": "npm run test -ws",
    "prepare": "node scripts/prepare.js",
    "pre-commit": ""
  },
  "publishConfig": {
    "registry": "https://tomtom.jfrog.io/artifactory/api/npm/maps-sdk-js-npm-local/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tomtom-international/maps-sdk-js.git"
  },
  "keywords": [
    "tomtom",
    "sdk",
    "api",
    "map",
    "location",
    "search",
    "routing",
    "traffic"
  ],
  "author": "GO SDK JS Team",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/tomtom-international/maps-sdk-js/issues"
  },
  "homepage": "https://github.com/tomtom-international/maps-sdk-js#readme",
  "workspaces": [
    "core",
    "services",
    "map",
    "map-integration-tests"
  ],
  "dependencies": {
    "@types/geojson": "^7946.0.16",
    "@types/lodash": "^4.17.20",
    "globals": "^16.3.0",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.1.2",
    "@size-limit/file": "^11.2.0",
    "@size-limit/preset-app": "^11.2.0",
    "@types/uuid": "^10.0.0",
    "cspell": "^9.2.0",
    "handlebars": "^4.7.8",
    "husky": "^8.0.3",
    "size-limit": "^11.2.0",
    "typedoc": "^0.28.7",
    "typedoc-plugin-missing-exports": "^4.0.0",
    "webpack-dev-server": "^5.2.2",
    "yaml": "^2.8.0"
  },
  "size-limit": [
    {
      "path": "services/dist/services*.js"
    },
    {
      "path": "core/dist/core*.js"
    },
    {
      "path": "map/dist/map*.js"
    }
  ],
  "engines": {
    "node": ">=22"
  }
}
