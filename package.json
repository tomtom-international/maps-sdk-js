{
  "name": "@cet/maps-sdk-js",
  "version": "0.8.24",
  "description": "JavaScript library for TomTom maps and location services",
  "scripts": {
    "clean": "pnpm -r --no-bail clean:dist && pnpm -r --no-bail clean:cache",
    "release:dry": "npx standard-version --dry-run",
    "release": "./scripts/release.sh && git push --follow-tags origin main",
    "build:api-reference": "typedoc",
    "build": "pnpm -F core build && pnpm -F services build && pnpm -F map build",
    "build:examples": "pnpm -r --filter='./examples/*' build",
    "type-check": "pnpm -r --no-bail type-check",
    "lint": "biome lint .",
    "lint:fix": "biome check . --write && biome lint . --write",
    "format": "biome format .",
    "format:fix": "biome format . --write",
    "test": "pnpm -F core test && pnpm -F services test && pnpm -F map test",
    "test:coverage": "pnpm -F core test:coverage && pnpm -F services test:coverage && pnpm -F map test:coverage",
    "pre-commit": ""
  },
  "publishConfig": {
    "registry": "https://artifactory.tomtom.com/artifactory/api/npm/maps-sdk-js-npm-local/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tomtom-international/maps-sdk-js.git"
  },
  "keywords": [
    "tomtom",
    "sdk",
    "api",
    "map",
    "location",
    "search",
    "routing",
    "traffic"
  ],
  "author": "TomTom International B.V.",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/tomtom-international/maps-sdk-js/issues"
  },
  "homepage": "https://github.com/tomtom-international/maps-sdk-js#readme",
  "dependencies": {
    "@types/lodash": "^4.17.20",
    "globals": "^16.3.0",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.2.2",
    "@size-limit/file": "^11.2.0",
    "@size-limit/preset-app": "^11.2.0",
    "@types/uuid": "^10.0.0",
    "cspell": "^9.2.0",
    "handlebars": "^4.7.8",
    "size-limit": "^11.2.0",
    "typedoc": "^0.28.11",
    "typedoc-plugin-missing-exports": "^4.1.0",
    "yaml": "^2.8.1"
  },
  "size-limit": [
    {
      "path": "services/dist/services*.js"
    },
    {
      "path": "core/dist/core*.js"
    },
    {
      "path": "map/dist/map*.js"
    }
  ],
  "engines": {
    "node": ">=22",
    "pnpm": ">=10"
  },
  "packageManager": "pnpm@10.15.0",
  "files": [
    "core/dist",
    "core/main.js",
    "core/package.json",
    "map/dist",
    "map/main.js",
    "map/package.json",
    "services/dist",
    "services/main.js",
    "services/package.json",
    "package.json",
    "README.md",
    "CHANGELOG.md",
    "LICENSE"
  ]
}
