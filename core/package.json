{
  "name": "core",
  "description": "",
  "main": "main.js",
  "module": "./dist/core.es.js",
  "scripts": {
    "test": "jest",
    "test:coverage": "jest --coverage --no-cache",
    "build": "rollup -c --configPlugin typescript",
    "build:full": "npm run type-check && npm run build",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "format": "prettier --check \"src/**/*\"",
    "format:fix": "prettier --write \"src/**/*\"",
    "type-check": "tsc --noEmit",
    "update-dependencies": "npx npm-check-updates -u --packageFile package.json",
    "clean:dist": "rimraf ./dist",
    "clean:cache": "rimraf ./.rollup.cache",
    "clean": "npm run clean:dist && npm run clean:cache"
  },
  "types": "./dist/index.d.ts",
  "author": "GO SDK JS Team",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tomtom-international/maps-sdk-js.git"
  },
  "bugs": {
    "url": "https://github.com/tomtom-international/maps-sdk-js/issues"
  },
  "homepage": "https://github.com/tomtom-international/maps-sdk-js#readme",
  "dependencies": {
    "geojson": "^0.5.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.4",
    "@rollup/plugin-node-resolve": "^15.2.1",
    "@rollup/plugin-replace": "^5.0.2",
    "@rollup/plugin-terser": "^0.4.3",
    "@rollup/plugin-typescript": "^11.1.3",
    "@types/geojson": "^7946.0.10",
    "@types/jest": "^29.5.4",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-jest": "^27.2.3",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.0.3",
    "rimraf": "^5.0.1",
    "rollup": "^3.29.1",
    "rollup-plugin-analyzer": "^4.0.0",
    "rollup-plugin-includepaths": "^0.2.4",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "typedoc": {
    "entryPoint": "./index.ts"
  }
