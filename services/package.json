{
  "name": "services",
  "description": "TomTom Maps SDK JS - bundle for service GeoJSON clients",
  "main": "main.js",
  "module": "./dist/services.es.js",
  "sideEffects": false,
  "scripts": {
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "build": "vite build",
    "build:full": "pnpm type-check && pnpm build",
    "build:watch": "pnpm build --watch",
    "type-check": "tsc --noEmit",
    "clean:dist": "rimraf ./dist"
  },
  "types": "./dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tomtom-international/maps-sdk-js.git"
  },
  "bugs": {
    "url": "https://github.com/tomtom-international/maps-sdk-js/issues"
  },
  "homepage": "https://github.com/tomtom-international/maps-sdk-js#readme",
  "devDependencies": {
    "@microsoft/api-extractor": "catalog:",
    "@rollup/plugin-terser": "catalog:",
    "@types/geojson": "catalog:",
    "@types/lodash-es": "catalog:",
    "@types/node": "catalog:",
    "@vitest/coverage-v8": "catalog:",
    "rimraf": "catalog:",
    "rollup-plugin-analyzer": "catalog:",
    "rollup-plugin-license": "catalog:",
    "rollup-plugin-visualizer": "catalog:",
    "shared-configs": "workspace:*",
    "ts-node": "catalog:",
    "typescript": "catalog:",
    "vite": "catalog:",
    "vite-plugin-dts": "catalog:",
    "vitest": "catalog:"
  },
  "typedoc": {
    "entryPoint": "./index.ts"
  },
  "dependencies": {
    "core": "workspace:*",
    "lodash-es": "catalog:",
    "zod": "^4.1.12"
  }
}
