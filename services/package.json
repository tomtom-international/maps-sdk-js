    "eslint-plugin-prettier": "^5.5.0",
    "jest": "^29.7.0",
    "prettier": "^3.5.3",
    "rimraf": "^6.0.1",
    "rollup": "^4.44.0",
    "rollup-plugin-analyzer": "^4.0.0",
    "rollup-plugin-includepaths": "^0.2.4",
