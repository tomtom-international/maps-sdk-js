{
  "compilerOptions": {
    "declaration": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "allowJs": false,
    "esModuleInterop": true,
    "moduleResolution": "bundler",
    "strict": true,
    "useDefineForClassFields": false,
    "target": "ESNext",
    "module": "ESNext",
    "paths": {
      "@tomtom-org/maps-sdk/core": ["../core"]
    }
  },
  "exclude": ["node_modules", "dist"]
}
