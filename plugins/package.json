{
  "name": "plugins",
  "version": "0.41.1",
  "type": "module",
  "description": "Plugins for TomTom Maps SDK JS",
  "scripts": {
    "build": "pnpm --parallel -F './*' build",
    "test": "vitest run",
    "test:e2e": "pnpm exec playwright install --with-deps chromium && playwright test",
    "test:e2e:ui": "pnpm exec playwright install --with-deps chromium && playwright test --ui",
    "test:e2e:update-snapshots": "pnpm exec playwright install --with-deps chromium && playwright test --update-snapshots",
    "type-check": "pnpm --parallel -F './*' type-check",
    "start-test-server": "npx serve . -p 9050"
  },
  "dependencies": {},
  "devDependencies": {
    "@microsoft/api-extractor": "catalog:",
    "@playwright/test": "catalog:",
    "@tsconfig/recommended": "catalog:",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "catalog:",
    "@types/react": "catalog:",
    "@types/react-dom": "catalog:",
    "@vitejs/plugin-react": "catalog:",
    "js-yaml": "^4.1.1",
    "rollup-plugin-analyzer": "catalog:",
    "rollup-plugin-license": "catalog:",
    "rollup-plugin-visualizer": "catalog:",
    "vite": "catalog:",
    "vite-plugin-dts": "catalog:",
    "vite-plugin-singlefile": "catalog:",
    "vitest": "catalog:"
  }
}
