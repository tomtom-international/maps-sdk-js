{
  "entryPoints": ["./core", "./map", "./services"],
  "out": "documentation/docs-portal/api-reference",
  "readme": "none",
  "name": "TomTom Maps for JavaScript",
  "exclude": ["**/*.test.ts"],
  "compilerOptions": {
    "skipLibCheck": true,
    "jsx": "preserve"
  },
  "externalSymbolLinkMappings": {
    "maplibre-gl": {
      "Map": "https://maplibre.org/maplibre-gl-js-docs/api/map/",
      "MapOptions": "https://maplibre.org/maplibre-gl-js-docs/api/map/",
      "*": "https://maplibre.org/maplibre-gl-js-docs/api/"
    },
    "@types/geojson": {
      "*": "https://github.com/DefinitelyTyped/DefinitelyTyped/blob/master/types/geojson/index.d.ts"
    }
  },
  "githubPages": false,
  "disableSources": true,
  "navigation": {
    "includeGroups": true,
    "compactFolders": false
  }
}
